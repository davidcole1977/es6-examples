{
  "name": "es6-examples",
  "version": "1.0.0",
  "description": "An area to explore new ES6 features, through self contained examples and katas",
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/mocha/bin/mocha ./examples/**/*.test.js --compilers js:babel-register --recursive",
    "test:watch": "npm run test -- --watch",
    "lint": "eslint ./src/*.js ./examples/**/*.js",
    "build": "webpack --progress --colors --watch --display-error-details",
    "serve": "node server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davidcole1977/es6-examples.git"
  },
  "author": "David Cole",
  "license": "ISC",
  "homepage": "https://github.com/davidcole1977/es6-examples#readme",
  "devDependencies": {
    "chalk": "1.1.3",
    "express": "4.4.5",
    "eslint": "~2.7.0",
    "accounting": "^0.4.1",
    "babel-eslint": "~6.0.2",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.5.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-register": "^6.5.2",
    "chai": "^3.5.0",
    "mocha": "^2.4.5",
    "ramda": "^0.19.1",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0",
    "webpack": "^1.12.15"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  }
